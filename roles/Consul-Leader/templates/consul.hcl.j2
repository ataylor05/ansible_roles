datacenter = "{{ datacenter_name }}"
data_dir = "/opt/consul"
encrypt = "{{ gossip_encryption_key }}"
ca_file = "/etc/consul-certs/consul-agent-ca.pem"
cert_file = "/etc/consul-certs/{{ datacenter_name }}-server-consul-0.pem"
key_file = "/etc/consul-certs/{{ datacenter_name }}-server-consul-0-key.pem"
verify_incoming = true
verify_outgoing = true
verify_server_hostname = true
retry_join = ["{{ retry_join_address }}"]

performance {
  raft_multiplier = 1
}